                        grid.dataSource.read();
                    } else {
                        $('#success_msg').show();
                        $('#error_msg').hide();
                        $('#labelIdd').text(response.responseText);
                        $("#success_msg").fadeTo(3000, 500).slideUp(500, function () {
                            $("#success_msg").slideUp(500);
                        });
                    }
                }
